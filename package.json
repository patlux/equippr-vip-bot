{
  "name": "equippr-vip-bot",
  "version": "1.0.0",
  "description": "Equippr bot which collects coins for you.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --detectOpenHandles src/",
    "check-types": "tsc --noEmit",
    "prettier": "prettier --ignore-path .gitignore \"./**/*.+(js|json|ts)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "concurrently -c \"cyan.bold,green.bold,blue.bold\" \"npm run check-types\" \"npm run check-format\" \"npm run test\"",
    "compile": "node scripts/build.js",
    "watch": "npm run compile -- --watch",
    "dev": "npm run compile && concurrently --kill-others -c \"cyan.bold,green.bold\" \"npm run watch\" \"nodemon -r dotenv/config --watch ./lib/\"",
    "build": "npm run check-types && npm run compile",
    "start": "npm run compile && node -r dotenv/config lib/index.js",
    "prepare": "husky install"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "camelcase": "^6.2.0",
    "chalk": "^4.1.1",
    "date-fns": "^2.19.0",
    "discord.js": "^12.5.1"
  },
  "devDependencies": {
    "@patwoz/prettier-config": "^1.1.0",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.32",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "esbuild": "^0.9.3",
    "esbuild-node-externals": "^1.0.3",
    "esbuild-runner": "^1.3.2",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.2.4"
  },
  "prettier": "@patwoz/prettier-config"
}
